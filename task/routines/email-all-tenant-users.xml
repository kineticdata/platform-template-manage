has_multiple_spaces = spaces.size &gt; 1

existing_links = spaces.collect {|space|
  "&lt;li&gt;Space Name: #{space['name']} (#{space['slug']}): https://kinops.io/#{space['slug']}&lt;/li&gt;"
}.join
new_links = spaces.collect {|space|
  "&lt;li&gt;Space Name: #{space['name']} (#{space['slug']}): https://#{space['slug']}.kinops.io&lt;/li&gt;"
}.join
if has_multiple_spaces
