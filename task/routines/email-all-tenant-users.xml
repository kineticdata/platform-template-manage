                <parameters>
                    <parameter id="input" label="Input" menu="" required="true" tooltip="">&lt;%
users_spaces = {}
@results['Space Users'].each do |i,v|
  space = JSON.parse(@results['Space'][i]['output'])
  space_users = JSON.parse(v['Result'])
  space_users.each do |user|
    if users_spaces.has_key?(user['email'])
       if !users_spaces[user['email']].include?(space)
          users_spaces[user['email']].push(space)
       end
    else 
       users_spaces[user['email']] = [space]
    end
  end
end
%&gt;
&lt;users&gt;
  &lt;%= users_spaces.collect{|email, spaces|
     "&lt;user&gt;#{{'email' =&gt; email, 'spaces' =&gt; spaces }.to_json}&lt;/user&gt;"
  }.join %&gt;
&lt;/users&gt;
</parameter>
