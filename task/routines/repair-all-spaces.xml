                <deferrable>false</deferrable>
                <visible>true</visible>
                <parameters>
                    <parameter id="input" label="Input" menu="" required="true" tooltip="">&lt;%=
    # Parse Repairable Spaces JSON
    spaces = JSON.parse(@results['Retrieve Repairable Spaces']['Environments'])
    # Prepare space slugs array
    space_slugs = @results['Manually Prioritized Space Slugs']['output'].split(',')
    # Loop over each space and add the space slug to the list
    spaces.each do |space|
      space_slugs &lt;&lt; space['values']['Space Slug']
    end
    # Ensure only unique space slugs
    space_slugs.uniq!
    # Delete any manually skipped spaces
    @results['Manually Skipped Space Slugs']['output'].split(',').each do |space|
      space_slugs.delete(space)
    end
    # Returned the comma-separated slugs
    space_slugs.join(',')
%&gt;
</parameter>
